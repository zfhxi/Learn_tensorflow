   "source": [
    "import tensorflow as tf\n",
    "a=tf.constant(1.)\n",
    "b=tf.constant(2.)\n",
    "c=tf.constant(3.)\n",
    "w=tf.constant(4.)\n",
    "\n",
    "with tf.GradientTape() as tape:\n",
    "    tape.watch([w])\n",
    "    y=a*w**2+b*w+c\n",
    "\n",
    "[dy_dw]=tape.gradient(y,[w])\n",
    "print(dy_dw)"
   ]
  }
